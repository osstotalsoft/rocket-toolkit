{
  "name": "@totalsoft/message-bus",
  "description": "Opinionated, provider independent, messaging patterns",
  "version": "2.4.6",
  "author": "TotalSoft",
  "homepage": "https://github.com/osstotalsoft/rocket-toolkit/tree/main/packages/message-bus",
  "packageManager": "yarn@3.2.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "composite": true,
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=12.9.0"
  },
  "scripts": {
    "build": "run clean && tsc --project tsconfig.build.json && yarn copy-files",
    "lint": "eslint src/*.ts",
    "clean": "rimraf dist",
    "test": "jest --collectCoverage --passWithNoTests --verbose --silent=false --runInBand",
    "tslint": "tslint -p tsconfig.json \"/**/*.ts\"",
    "rusi-sample": "node src/___samples/rusi.js",
    "copy-files": "copyfiles -u 1 src/**/*.proto dist/src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/osstotalsoft/rocket-toolkit.git",
    "directory": "packages/message-bus"
  },
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.4.1",
    "@grpc/proto-loader": "^0.6.5",
    "async-mutex": "^0.3.1",
    "bluebird": "3.7.2",
    "humps": "^2.0.1",
    "node-nats-streaming": "0.3.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@jest/types": "^28.1.0",
    "@types/bluebird": "^3.5.37",
    "@types/copyfiles": "^2",
    "@types/humps": "^2.0.2",
    "@types/jest": "^27.5.1",
    "@types/node": "^18.7.22",
    "@types/rimraf": "^3",
    "@types/uuid": "^8.3.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "rimraf": "^3.0.2"
  }
}
